#include "main.c"

               {'s', print_s},
                {'d', print_int},
                {'i', print_int}
        };
        int i, length;

        for (i = 0; i < NUM_OF_SPECS; ++i)
        {
 	        if (spec == f_specs[i].spec)
                        length = f_specs[i].func(args, f);
        }

        return (length);
}

